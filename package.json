{
  "name": "dev-linker",
  "version": "1.4.1",
  "description": "虚拟4G模块模拟器 - 物联网开发调试工具",
  "main": "dist/main/main.js",
  "type": "commonjs",
  "author": "九秋",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "electron . --dev",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "preview": "electron .",
    "test:servers": "node test-server/start-all.js",
    "test:ws": "node test-server/ws-server.js",
    "test:tcp": "node test-server/tcp-server.js",
    "test:udp": "node test-server/udp-server.js",
    "test:mqtt": "node test-server/mqtt-server.js",
    "test:http": "node test-server/http-server.js",
    "lint": "eslint src --ext .ts,.vue",
    "type-check": "tsc --noEmit",
    "compile": "npm run build"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "electron": "^28.0.0",
    "element-plus": "^2.5.0",
    "pinia": "^2.1.7",
    "playwright": "^1.56.1",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "aedes": "^0.51.3",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "mqtt": "^5.3.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "ws": "^8.18.3"
  }
}
